<h1>Users.erb</h1>
<table class="table table-hover">
    <thead>
        <th>No</th>
        <th>ID</th>
        <th>Password</th>
    </thead>
    
    <tbody>
        <% @users.each_with_index do |user, index| %>
        <tr>
            <td><%=index+1 %></td>
            <td><%=user[0] %></td>
            <td><%=user[1] %></td>
        </tr>
        <%end%>
    </tbody>
    
</table>